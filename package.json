{
  "name": "ymty",
  "displayName": "ymty",
  "description": "",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.81.0"
  },
  "publisher": "ymty",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Ymty One Dark Pro Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Ymty One Dark Pro Theme-color-theme.json"
      },
      {
        "label": "Ymty Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Ymty Dark Theme-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "ymty.hello",
        "title": "Hello",
        "category": "ymty"
      },
      {
        "command": "ymty.copyName",
        "title": "Copy Name",
        "category": "ymty"
      },
      {
        "command": "ymty.copyFullName",
        "title": "Copy Full Name",
        "category": "ymty"
      },
      {
        "command": "ymty.config",
        "title": "Config",
        "category": "ymty"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ymty.copyName",
          "group": "6_copypath@30",
          "when": "isFileSystemResource"
        },
        {
          "command": "ymty.copyFullName",
          "group": "6_copypath@40",
          "when": "isFileSystemResource"
        }
      ],
      "editor/title/context": [
        {
          "command": "ymty.copyName",
          "group": "1_cutcopypaste@3",
          "when": "isFileSystemResource"
        },
        {
          "command": "ymty.copyFullName",
          "group": "1_cutcopypaste@4",
          "when": "isFileSystemResource"
        }
      ]
    },
    "configuration": {
      "title": "ymty",
      "properties": {
        "ymty.hello": {
          "type": "string",
          "default": "waht a beautiful day",
          "description": "hello"
        },
        "yourPlugin.open": {
          "type": "boolean",
          "default": true,
          "description": "服务开关"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "fs-extra": "^11.1.1"
  },
  "homepage": "https://github.com/ymty/ymty-vsc-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/ymty/ymty-vsc-tools"
  },
  "bugs": {
    "url": "https://github.com/ymty/ymty-vsc-tools/issues"
  }
}