{
  "name": "ymty",
  "displayName": "ymty",
  "description": "",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.81.0"
  },
  "publisher": "ymty",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Ymty One Dark Pro Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Ymty One Dark Pro Theme-color-theme.json"
      },
      {
        "label": "Ymty Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Ymty Dark Theme-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "ymty.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ymty.copyName",
        "title": "Copy Name"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ymty.copyName",
          "group": "6_copypath@30",
          "when": "isFileSystemResource"
        }
      ],
      "editor/title/context": [
        {
          "command": "ymty.copyName",
          "group": "1_cutcopypaste@3",
          "when": "isFileSystemResource"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "homepage": "https://github.com/ymty/ymty-vsc-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/ymty/ymty-vsc-tools"
  },
  "bugs": {
    "url": "https://github.com/ymty/ymty-vsc-tools/issues"
  }
}